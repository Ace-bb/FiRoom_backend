.container {
  width: 100%;
  height: 100%;
  padding: 16rpx;
  box-sizing: border-box;
}
.container .Match-swiper {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.container .Match-swiper .notice_swiper {
  width: 100%;
  height: 800rpx;
  overflow: hidden;
}
.container .Match-swiper .notice_swiper .notice_itemr {
  width: 100%;
  height: 100%;
}
.container .match-detail-title {
  width: 100%;
  font-size: 32rpx;
  font-weight: bold;
  padding-left: 12rpx;
  box-sizing: border-box;
  overflow: hidden;
  text-overflow: ellipsis;
  max-lines: 2;
}
.container .match-detail-wrap {
  width: 100%;
  padding: 12rpx;
  box-sizing: border-box;
}
.container .match-detail-wrap .match-detail-wrap {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.container .match-detail-wrap .tags-wrap {
  width: 100%;
  margin-top: 16rpx;
}
.container .match-detail-wrap .tags-wrap .tag {
  color: rgba(29, 143, 236, 0.644);
}
.container .match-detail-wrap .wrap-bottom {
  width: 100%;
  padding-top: 16rpx;
  padding-bottom: 48rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid rgba(169, 169, 169, 0.692);
}
.container .match-detail-wrap .wrap-bottom .edit-time {
  font-size: 26rpx;
  font-weight: lighter;
}
.container .comment-wrap {
  width: 100%;
  display: flex;
  flex-direction: column;
  padding: 16rpx;
  box-sizing: border-box;
}
.container .comment-wrap .comment-number {
  padding-top: 36rpx;
  padding-bottom: 16rpx;
  font-size: 26rpx;
  font-weight: lighter;
  border-bottom: 1rpx solid rgba(169, 169, 169, 0.164);
}
.container .comment-wrap .user-comment-wrap {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 12rpx;
  border-bottom: 1rpx solid ;
}
.container .comment-wrap .user-comment-wrap .user-icon {
  width: 64rpx;
  height: 64rpx;
  border-radius: 100rpx;
}
.container .comment-wrap .user-comment-wrap .user-input-comment {
  width: 100%;
  height: 72rpx;
  margin-left: 24rpx;
  background-color: rgba(109, 106, 106, 0.062);
  border-radius: 100rpx;
  padding-right: 24rpx;
  box-sizing: border-box;
}
.container .comment-wrap .other-comment-wrap {
  width: 100%;
  margin-top: 36rpx;
}
.container .comment-wrap .other-comment-wrap .other-comment {
  width: 100%;
  border-bottom: 1rpx solid rgba(128, 128, 128, 0.342);
  display: flex;
  flex-direction: row;
  align-items: center;
  padding-top: 12rpx;
  padding-bottom: 12rpx;
  box-sizing: border-box;
}
.container .comment-wrap .other-comment-wrap .other-comment .icon-wrap {
  width: 64rpx;
  height: 100%;
}
.container .comment-wrap .other-comment-wrap .other-comment .icon-wrap .other-icon {
  width: 64rpx;
  height: 64rpx;
  border-radius: 100rpx;
}
.container .comment-wrap .other-comment-wrap .other-comment .main-box {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-left: 16rpx;
  justify-content: space-between;
  margin-top: 24rpx;
}
.container .comment-wrap .other-comment-wrap .other-comment .main-box .usernamne {
  width: 100%;
  height: 36rpx;
  font-size: 28rpx;
  font-weight: lighter;
}
.container .comment-wrap .other-comment-wrap .other-comment .main-box .comment {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
}
.container .comment-wrap .other-comment-wrap .other-comment .main-box .comment-time {
  width: 100%;
  font-size: 24rpx;
  font-weight: lighter;
}
.container .comment-wrap .other-comment-wrap .other-comment .like-wrap {
  width: 48rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  margin-top: 24rpx;
}
.container .comment-wrap .other-comment-wrap .other-comment .like-wrap .like-icon {
  width: 40rpx;
  height: 40rpx;
}
.container .comment-wrap .other-comment-wrap .other-comment .like-wrap .like-num {
  font-size: 22rpx;
}

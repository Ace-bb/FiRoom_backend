page,
.page {
    height: 100%;
    font-family: 'PingFang SC', 'Helvetica Neue', Helvetica, 'Droid Sans Fallback', 'Microsoft Yahei', sans-serif;
}

.index {
    font-family: "微软雅黑";
    flex: 1;
    min-height: 100%;
    font-size: 28rpx;
    height: 100%;
    position: fixed;
    width: 750rpx;
    background-color: #f7f5ec;
    z-index: 0;
    transition: All 0.4s ease;
    -webkit-transition: All 0.4s ease;
}

.index-topcon {
    width: 100%;
    height: 130rpx;
    left: 0;
    z-index: 200;
}

.index-search {
    width: 720rpx;
    height: 80rpx;
    border-radius: 8rpx;
    background-color: #f94769;
    position: absolute;
    left: 0;
    right: 0;
    top: 22rpx;
    margin: auto;
}

.category_im {
    width: 40rpx;
    height: 40rpx;
    margin-left: 18rpx;
    margin-top: 18rpx;
    position: absolute;
}

.search-ipt {
    width: 472rpx;
    border-radius: 8rpx 0 0 8rpx;
    padding-left: 72rpx;
    float: left;
    font-family: "微软雅黑";
    height: 72rpx;
    margin-top: 2rpx;
    margin-left: 68rpx;
    background-color: #fff;
    position: absolute;
    z-index: 1;
    color: #333;
}

.search-btn {
    width: 110rpx;
    height: 70rpx;
    line-height: 70rpx;
    float: right;
    text-align: center;
    color: #fff;
    font-size: 30rpx;
    position: absolute;
    right: 0;
    z-index: 10;
    background-color: #f94769;
}

.search-icon {
    width: 40rpx;
    height: 40rpx;
    position: absolute;
    left: 78rpx;
    top: 18rpx;
    z-index: 10;
}

.index-camera {
    width: 56rpx;
    height: 66rpx;
    position: absolute;
    right: 110rpx;
    top: 0;
    margin-top: 4rpx;
    z-index: 10;
}

.camera-icon {
    width: 40rpx;
    height: 35rpx;
    margin-top: 15rpx;
}

.camera-file {
    display: block;
    width: 100%;
    height: 100%;
    position: absolute;
    opacity: 0;
}

.camera-image {
    position: absolute;
    width: 68rpx;
    height: 56rpx;
    top: 7rpx;
    left: 5rpx;
    z-index: 20;
    border: 1px solid #ccc;
    background-color: #fff;
}

.search-image {
    width: 40rpx;
    height: 56rpx;
    float: left;
}

.search-cha {
    width: 15rpx;
    float: left;
    height: 15rpx;
    margin-top: 24rpx;
    margin-left: 8rpx;
}


/*产品*/

.procon {
    display: inline-block;
    padding: 20rpx;
}

.procon-imgmes {
    float: left;
    width: 350rpx;
    height: 555rpx;
    background-color: #fff;
    margin-right: 10rpx;
    margin-bottom: 10rpx;
}

.procon-imgmes:nth-child(2n) {
    margin-right: 0!important;
}

.imgmes-img {
    display: block;
    width: 350rpx;
    height: 450rpx;
}

.imgmes-mes1 {
    display: block;
    width: 310rpx;
    height: 50rpx;
    line-height: 50rpx;
    margin-left: 20rpx;
    color: #333;
    font-size: 30rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.imgmes-mes2 {
    display: block;
    width: 310rpx;
    height: 40rpx;
    line-height: 40rpx;
    margin-left: 20rpx;
    color: #333;
    font-size: 30rpx;
}

.mes1 {
    float: left;
    color: #999;
    font-size: 24rpx;
}

.mes2 {
    float: right;
    color: #ff1a1a;
    font-size: 28rpx;
}

.box-bottom-wrap {
    width: 100%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 12rpx;
    box-sizing: border-box;
}

.box-bottom-wrap .author-wrap {
    height: 48rpx;
    display: flex;
    margin-left: 4rpx;
}

.box-bottom-wrap .author-wrap .author-icon {
    width: 44rpx;
    height: 44rpx;
    border-radius: 100rpx;
}

.box-bottom-wrap .author-wrap .author-name {
    height: 100%;
    font-size: 30rpx;
    margin-left: 24rpx;
    font-weight: lighter;
    display: flex;
    align-items: center;
    justify-content: center;
}

.box-bottom-wrap .like-wrap {
    height: 48rpx;
    display: flex;
    align-items: center;
}

.box-bottom-wrap .like-wrap .like-img {
    width: 36rpx;
    height: 36rpx;
    margin-right: 6rpx;
}

.box-bottom-wrap .like-wrap .like-num {
    font-size: 24rpx;
    font-weight: lighter;
    display: flex;
    align-items: center;
    justify-content: center;
}

.empty-con {
    width: 290rpx;
    height: 200rpx;
    margin: auto;
    margin-top: 180rpx;
    text-align: center;
}

.empty-image {
    display: block;
    width: 120rpx;
    height: 120rpx;
    margin: auto;
    margin-bottom: 20rpx;
}

.empty-text {
    display: block;
}

.tipsImg {
    width: 40rpx;
    height: 40rpx;
    display: block;
    margin: auto;
}

.tipsText {
    line-height: 80rpx;
    text-align: center;
}

.page-bottom {
    height: 100%;
    width: 320rpx;
    position: fixed;
    z-index: 0;
}

.wc {
    color: rgb(99, 18, 18);
    padding: 30rpx 0 30rpx 40rpx;
    font-size: 28rpx;
    border: solid rgb(173, 167, 167);
    border-width: 3rpx 0rpx 3rpx 0rpx;
}

.page-content {
    padding-top: 30rpx;
}

.c-state1 {
    transform: rotate(0deg) scale(1) translate(28%, 0%);
    -webkit-transform: rotate(0deg) scale(1) translate(28%, 0%);
}

.text {
    margin: auto;
    margin-top: 20rpx;
    margin-left: 130rpx;
    vertical-align: middle;
    color: white;
}
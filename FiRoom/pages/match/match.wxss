.container {
  background-color: #F2f2f2;
  min-height: 100%;
}
.container .index-topcon {
  width: 100%;
  height: 120rpx;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
  position: relative;
}
.container .index-topcon .index-search {
  width: 706rpx;
  height: 76rpx;
  padding: 2rpx;
  border-radius: 8rpx;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 10rpx;
  margin: auto;
  background-color: #f94769;
}
.container .index-topcon .index-search .search-ipt {
  width: 500rpx;
  border-radius: 8rpx 0 0 8rpx;
  padding-left: 78rpx;
  float: left;
  font-family: "微软雅黑";
  height: 72rpx;
  margin-top: 2rpx;
  margin-left: 2rpx;
  background-color: #fff;
  position: absolute;
  z-index: 1;
  color: #333;
}
.container .index-topcon .index-search .search-btn {
  width: 120rpx;
  height: 76rpx;
  line-height: 76rpx;
  text-align: center;
  color: #fff;
  font-size: 36rpx;
  position: absolute;
  top: 2rpx;
  right: 10rpx;
  z-index: 10;
  background-color: #f94769;
}
.container .index-topcon .index-search .search-icon {
  width: 35rpx;
  height: 35rpx;
  position: absolute;
  left: 26rpx;
  top: 20rpx;
  z-index: 10;
}
.container .index-topcon .index-search .index-camera {
  width: 50rpx;
  height: 76rpx;
  position: absolute;
  right: 140rpx;
  top: 0;
  z-index: 10;
}
.container .index-topcon .index-search .index-camera .camera-file {
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  opacity: 0;
}
.container .index-topcon .index-search .index-camera .camera-icon {
  width: 40rpx;
  height: 35rpx;
  margin-top: 20rpx;
}
.container .swiper-container {
  width: 750rpx;
  position: relative;
}
.container .swiper-container .swiper_box {
  width: 100%;
  height: 360rpx;
}
.container .swiper-container .swiper_box .slide-image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height: 100%;
}
.container .swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}
.container .swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 50%;
  transition: all .6s;
  opacity: .5;
}
.container .type-container {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  overflow: hidden;
}
.container .type-container .type-box {
  width: 160rpx;
  box-sizing: content-box;
  font-size: 24rpx;
  line-height: 84rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.container .type-container .type-box .type-navbar-item {
  border-bottom: 4rpx solid #fff;
}
.container .type-container .type-box .type-item-on {
  border-bottom: 4rpx solid #e64340;
}
.container .notice {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  height: 80rpx;
  line-height: 80rpx;
  width: 750rpx;
  margin-top: 10rpx;
  background-color: #fff;
}
.container .notice .notice_iteml {
  padding-left: 25rpx;
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  font-size: 26rpx;
  width: 80rpx;
  height: 50rpx;
  line-height: 50rpx;
  color: #999;
}
.container .notice .notice_swiper {
  padding-top: 15rpx;
  padding-bottom: 15rpx;
  height: 50rpx;
  line-height: 50rpx;
  width: 635rpx;
}
.container .notice .notice_swiper .notice_itemr {
  padding-left: 10rpx;
  font-size: 26rpx;
  overflow: hidden;
  color: #e64340;
  width: 635rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.container .goods-container {
  width: 100%;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  box-sizing: border-box;
  padding: 24rpx;
}
.container .goods-container .title-wrap {
  width: 100%;
  display: flex;
  align-items: center;
  padding-bottom: 16rpx;
  margin-bottom: 16rpx;
}
.container .goods-container .title-wrap .line {
  width: 40%;
  border-bottom: 1rpx solid rgba(128, 128, 128, 0.459);
}
.container .goods-container .title-wrap .title {
  font-size: 26rpx;
  font-weight: lighter;
}
.container .goods-container .goods-box {
  width: 49%;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 24rpx;
}
.container .goods-container .goods-box .img-box {
  width: 100%;
  overflow: hidden;
}
.container .goods-container .goods-box .img-box .image {
  width: 100%;
}
.container .goods-container .goods-box .goods-title {
  width: 280rpx;
  height: 64rpx;
  text-overflow: ellipsis;
  max-lines: 2;
  white-space: wrap;
  overflow: hidden;
  font-size: 24rpx;
  padding: 24rpx 0 0rpx 0;
  color: #000;
  margin-left: 24rpx;
}
.container .goods-container .goods-box .box-bottom-wrap {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 12rpx;
  box-sizing: border-box;
}
.container .goods-container .goods-box .box-bottom-wrap .author-wrap {
  height: 48rpx;
  display: flex;
  margin-left: 4rpx;
}
.container .goods-container .goods-box .box-bottom-wrap .author-wrap .author-icon {
  width: 44rpx;
  height: 44rpx;
  border-radius: 100rpx;
}
.container .goods-container .goods-box .box-bottom-wrap .author-wrap .author-name {
  height: 100%;
  font-size: 30rpx;
  margin-left: 24rpx;
  font-weight: lighter;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .goods-container .goods-box .box-bottom-wrap .like-wrap {
  height: 48rpx;
  display: flex;
  align-items: center;
}
.container .goods-container .goods-box .box-bottom-wrap .like-wrap .like-img {
  width: 36rpx;
  height: 36rpx;
  margin-right: 6rpx;
}
.container .goods-container .goods-box .box-bottom-wrap .like-wrap .like-num {
  font-size: 24rpx;
  font-weight: lighter;
  display: flex;
  align-items: center;
  justify-content: center;
}
.container .goods-container .question-box {
  width: 100%;
  border-bottom: 1rpx solid black;
}
.container .goods-container .question-box .poster-head-box {
  width: 100%;
  height: 84rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 6rpx;
  box-sizing: border-box;
}
.container .goods-container .question-box .poster-head-box .head-img {
  width: 64rpx;
  height: 64rpx;
}
.container .goods-container .question-box .poster-head-box .poster-name-box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 24rpx;
}
.container .goods-container .question-box .poster-head-box .poster-name-box .poster-name {
  font-size: 24rpx;
}
.container .goods-container .question-box .poster-head-box .poster-name-box .post-time {
  font-size: 16rpx;
}
.container .goods-container .question-box .match-message-box {
  width: 100%;
}
.container .goods-container .question-box .match-message-box .image-wrap {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  align-items: center;
}
.container .goods-container .question-box .match-message-box .image-wrap .images-box {
  width: 30%;
  height: 360rpx;
  margin-top: 24rpx;
}
.container .goods-container .question-box .match-message-box .image-wrap .images-box .match-image {
  width: 100%;
  height: 100%;
}
.container .goods-container .question-box .master-unswer-wrap {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 12rpx;
  box-sizing: border-box;
}
.container .goods-container .question-box .master-unswer-wrap .master-box {
  width: 80%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
  border: 1rpx solid rgba(58, 56, 56, 0.342);
  padding: 12rpx;
  box-sizing: border-box;
}
.container .goods-container .question-box .master-unswer-wrap .master-box .master-head {
  width: 80rpx;
  height: 80rpx;
}
.container .goods-container .question-box .master-unswer-wrap .master-box .master-message-box {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  margin-left: 24rpx;
}
.container .goods-container .question-box .master-unswer-wrap .master-box .master-message-box .master-name {
  font-size: 32rpx;
}
.container .goods-container .question-box .master-unswer-wrap .master-box .master-message-box .unswer-message {
  font-size: 24rpx;
}
.container .goods-container .question-box .like-comment-wrap {
  width: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .goods-container .question-box .like-comment-wrap .view-count {
  font-size: 20rpx;
}
.container .goods-container .question-box .like-comment-wrap .like-comment {
  width: 15%;
  height: 64rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.container .goods-container .question-box .like-comment-wrap .like-comment .like {
  width: 48rpx;
  height: 48rpx;
}
.container .goods-container .question-box .like-comment-wrap .like-comment .comment {
  width: 48rpx;
  height: 48rpx;
}
.container .add-container {
  width: 108rpx;
  height: 108rpx;
  position: fixed;
  bottom: 0;
  right: 0;
  border-radius: 500rpx;
  margin-right: 12rpx;
  margin-bottom: 12rpx;
  box-shadow: 4rpx 4rpx 4rpx 4rpx rgba(32, 31, 31, 0.4);
}
.container .no-more-goods {
  text-align: center;
  font-size: 24rpx;
  padding-bottom: 48rpx;
  color: #999;
}
